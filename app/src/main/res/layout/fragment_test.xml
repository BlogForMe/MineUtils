<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <Button
        android:id="@+id/error_return"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OnErrorReturn()"
        android:textSize="@dimen/dimen_20" />

    <Button
        android:id="@+id/error_resume_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="onErrorResumeNext()"
        android:textSize="@dimen/dimen_20" />

    <Button
        android:id="@+id/test_retry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="testRetry()"
        android:textSize="@dimen/dimen_20" />

    <Button
        android:id="@+id/retry_when"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="retryWhen()"
        android:textSize="@dimen/dimen_20" />

</LinearLayout>
